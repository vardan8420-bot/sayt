{
  "name": "sayt",
  "version": "0.1.0",
  "description": "Modern web tools with currency converter, language switcher, image editor, file uploader, and SEO optimization",
  "private": true,
  "author": "Sayt Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vardan84/sayt.git"
  },
  "keywords": [
    "nextjs",
    "react",
    "typescript",
    "image-editor",
    "seo-optimizer",
    "web-tools",
    "currency-converter",
    "language-switcher",
    "file-uploader"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "next dev -p 3000 -H 0.0.0.0",
    "build": "prisma generate && next build",
    "postbuild": "prisma generate",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=0",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out",
    "icons": "node scripts/generate-icons.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:setup": "node scripts/setup-database.js",
    "db:auto": "node scripts/auto-migrate.js",
    "postinstall": "npm run db:generate",
    "test:e2e": "playwright test",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.65",
    "@hookform/resolvers": "^5.2.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.19.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.2.6",
    "@sentry/node": "^10.25.0",
    "@stripe/react-stripe-js": "^5.3.0",
    "@stripe/stripe-js": "^8.4.0",
    "@supabase/supabase-js": "^2.81.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/bcryptjs": "^2.4.6",
    "@uploadthing/react": "^7.3.3",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "ai": "^5.0.93",
    "autoprefixer": "^10.4.22",
    "bcryptjs": "^3.0.3",
    "instantsearch.js": "^4.83.0",
    "meilisearch": "^0.54.0",
    "next": "^16.0.3",
    "next-auth": "^4.24.13",
    "postcss": "^8.5.6",
    "prisma": "^6.19.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "stripe": "^19.3.1",
    "tailwindcss": "^4.1.17",
    "uploadthing": "^7.7.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.3",
    "eslint-plugin-react-hooks": "^7.0.1",
    "pg": "^8.16.3",
    "sharp": "^0.34.5",
    "typescript": "^5.9.3",
    "vitest": "^4.0.9"
  }
}
